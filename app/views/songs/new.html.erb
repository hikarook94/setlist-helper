<%= form_with model: @song do |form| %>
  <%= form.label :name, '曲名' %>
  <%= form.text_field :name %>
  <%= form.label :artist, 'アーティスト名' %>
  <%= form.text_field :artist %>
  <%= form.text_field :minutes %>
  <%= form.label :minutes, '分' %>
  <%= form.text_field :seconds %>
  <%= form.label :seconds, '秒' %>
  <%= form.submit '登録する' %>
<% end %>
